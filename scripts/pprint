#!/usr/bin/env python3
# Usage: cat example.json(l) | pprint
import sys
import json

def read_input():
    if len(sys.argv) > 1:
        with open(sys.argv[1], "r") as f:
            return f.read()
    return sys.stdin.read()

data = read_input().strip()

try:
    obj = json.loads(data)
    print(json.dumps(obj, indent=2))
    sys.exit(0)
except json.JSONDecodeError:
    pass

for line in data.splitlines():
    if not line.strip():
        continue
    obj = json.loads(line)
    print(json.dumps(obj, indent=2))
